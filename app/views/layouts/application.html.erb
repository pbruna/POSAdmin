<!DOCTYPE html>
<html>
<head>
  <title>POSAdmin</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div style="margin-bottom: 60px">
		<%= render "shared/header" %>
	</div>

	<div class="container-fluid">
		<div class="sidebar">
			<%= render :partial => "shared/sidebar" %>
		</div>
		<div class="content">
			<%= yield %>
		</div>
	</div>
</body>
</html>
